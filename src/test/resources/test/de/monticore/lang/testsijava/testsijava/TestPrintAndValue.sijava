package test.de.monticore.lang.testsijava.testsijava;

siclass TestPrintAndValue {

    /**
     * This method generates the following output
     * 20030.0m
     * 2.0h
     * 2.0
     * 12.0
     * 150.0
     * 7200.0
     * 7210.0
     * 9000.0
     * 1000.0km/h
     * 7200010.0ms
     */
    void main () {
        h var = 7200 s;

        // 20030.0m
        print(20 km + 30m);

        // 2.0h
        print (var);

        // 2.0
        print (value(var));

        // 12.0
        print (value(var) + 10);

        // 150.0
        print (value(var + 30 min));

        // 7200.0
        print (basevalue(var));

        // 7210.0
        print (basevalue(var) + 10);

        // 9000.0
        print (basevalue(var + 30 min));

        // 1000.0km/h
        print (2000 km / var);

        // 7200010.0ms
        print (var + 10 ms);
    }

}